<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%msg%n</pattern>
            <!-- <pattern>%d{yyyy-MM-dd HH:mm:ss} [%-10thread] %-5level %-4line %-34logger{32}: %msg%n</pattern> -->p
        </encoder>
    </appender>

    <logger name="com.makolab" level="INFO" />

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>